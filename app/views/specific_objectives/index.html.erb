<p id="notice"><%= notice %></p>

<h1>Specific Objectives</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @specific_objectives.each do |specific_objective| %>
      <tr>
        <td><%= specific_objective.name %></td>
        <td><%= specific_objective.comments %></td>
        <td><%= link_to 'Show', specific_objective %></td>
        <td><%= link_to 'Edit', edit_specific_objective_path(specific_objective) %></td>
        <td><%= link_to 'Destroy', specific_objective, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Specific Objective', new_specific_objective_path %>
